<h1>Listing feed_items</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Summary</th>
      <th>Url</th>
      <th>Published at</th>
      <th>Guid</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @feed_items.each do |feed_item| %>
      <tr>
        <td><%= feed_item.name %></td>
        <td><%= feed_item.summary %></td>
        <td><%= feed_item.url %></td>
        <td><%= feed_item.published_at %></td>
        <td><%= feed_item.guid %></td>
        <td><%= link_to 'Show', feed_item %></td>
        <td><%= link_to 'Edit', edit_feed_item_path(feed_item) %></td>
        <td><%= link_to 'Destroy', feed_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feed item', new_feed_item_path %>
